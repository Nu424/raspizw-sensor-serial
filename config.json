{
    "serial": {
        "port": "/dev/ttyGS0",
        "baudrate": 9600,
        "timeout": 1.0,
        "write_timeout": 1.0
    },
    "system": {
        "loop_interval": 0.1,
        "max_command_length": 256,
        "shutdown_timeout": 5.0
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "sensor_server.log",
        "max_bytes": 1048576,
        "backup_count": 3
    }
}